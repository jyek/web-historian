function checkGOLLogo() {

	var gapLogo = $('gapLogo');
	var gapLogoAssetPath = "/Asset_Archive/GPWeb/content/0007/794/394/assets/";
	
	var girlsDivState = $('division6256').className;
	var boysDivState = $('division6172').className;
	var tgDivState = $('division6413').className;
	var tbDivState = $('division6345').className;
	var bgDivState = $('division6487').className;
	
	/* divsion class _selected version */
	if ((girlsDivState.indexOf("_selected") >= 0)||(boysDivState.indexOf("_selected") >= 0)) {
		
		if(gapLogo) {
	 	gapLogo.src = gapLogoAssetPath + "GAPkids_logo_95px.gif";	  
		}	
		
	} else if ((tgDivState.indexOf("_selected") >= 0)||(tbDivState.indexOf("_selected") >= 0)||(bgDivState.indexOf("_selected") >= 0)) {
	
		if(gapLogo) {
	 	gapLogo.src = gapLogoAssetPath + "babyGAP_logo_95px.gif";	  
		}
	} 
	
}

