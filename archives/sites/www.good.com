<script type="text/javascript">
$(document).ready(function() {
// Added by MT
// Pop up the light box via AJAX call
	$('.video-link').click(function(e) {
		var videoObject = $(this);
		var videoID = videoObject.attr("id");
		var playlistID = videoObject.attr("playlist-id");
		var dataDivID = videoObject.attr("data-div-id");
		var title = videoObject.attr("title");
		var pageURL = window.location.href;
		//alert("video ID >> " + videoID);
		//alert("playlist ID >> " + playlistID);
		//alert("DIV " + dataDivID);
		//alert("Title " + title);
		var getVideoURL = '/templates/Good-Link_Lightbox_Helper?videoID='+videoID+'&playlistID='+playlistID+'&dataDivID='+dataDivID+'&title='+title+'&pageURL='+pageURL;
		jQuery.ajax({
			url: getVideoURL,
				cache: false,
				type: "GET",
				dataType: "html",
				success: function(data){
					//alert("success");
					//alert(data);
					//alert("Div id " +videoObject.attr('data-div-id'));					
					var divID = videoObject.attr("data-div-id");
					videoObject.after(data);
					// To Reinitialize the FB-Like Button
					FB.XFBML.parse(); 
					// To Reinitialize the Twitter Button
					twttr.widgets.load();
					
					$('body').css({'overflow':'hidden'});
					$('.overlay').show();
					//var divID =  $(this).attr("data-div-id");
					
					//alert("DIV " + divID);
					var container = $('#' + divID).children('.container').detach();
					//alert("container >>"+container);
					$('div.lightbox').html('').attr('data-div-id', divID);
					container.appendTo('div.lightbox');
					$('div.lightbox').show();
		}
		});
	});
});
</script>
